{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
	"@typescript-eslint"
  ],
  "extends": [
	"@specialblend/eslint-config",
	"plugin:@typescript-eslint/eslint-recommended",
	"plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
	"ecmaVersion": 6,
	"sourceType": "module",
	"ecmaFeatures": {
	  "modules": true
	},
	"env": {
	  "jest": true
	}
  }
}
